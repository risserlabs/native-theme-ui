{
  "name": "@multiplatform/expo",
  "version": "0.0.1",
  "private": true,
  "main": "index.js",
  "scripts": {
    "android": "expo start --android",
    "eject": "expo eject",
    "electron": "expo-electron start",
    "ios": "expo start --ios",
    "postinstall": "make -s postinstall",
    "start": "expo start",
    "web": "expo start --web"
  },
  "dependencies": {
    "-": "*",
    "@babel/runtime": "^7.18.6",
    "@dripsy/core": "^3.6.0",
    "core-js": "^3.23.3",
    "dripsy": "^3.6.0",
    "electron": "^17.4.8",
    "expo": "~45.0.6",
    "expo-dev-client": "~1.0.0",
    "expo-dev-launcher": "^0.10.6",
    "expo-status-bar": "~1.3.0",
    "react": "^17.0.2",
    "react-native": "0.68.2",
    "react-native-web": "0.17.7"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@expo/electron-adapter": "^0.0.55",
    "@expo/webpack-config": "^0.16.25",
    "@types/prettier": "^2.6.3",
    "@types/react": "^18.0.14",
    "@types/react-native": "^0.66.19",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "babel-preset-expo": "^9.1.0",
    "cspell": "^6.1.3",
    "electron-windows-store": "^2.1.0",
    "eslint": "^8.18.0",
    "eslint-config-next": "^12.2.0",
    "eslint-plugin-react": "^7.30.1",
    "expo-dev-client": "~1.0.0",
    "expo-yarn-workspaces": "^2.0.0",
    "jest": "^28.1.2",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "expo-yarn-workspaces": {
    "symlinks": [
      "@expo/next-adapter",
      "@sentry/cli",
      "@sentry/react-native",
      "@storybook/react",
      "@storybook/react-native-server",
      "@unimodules/core",
      "@unimodules/react-native-adapter",
      "expo-app-loading",
      "expo-application",
      "expo-constants",
      "expo-dev-client",
      "expo-dev-launcher",
      "expo-dev-menu",
      "expo-dev-menu-interface",
      "expo-device",
      "expo-error-recovery",
      "expo-file-system",
      "expo-font",
      "expo-haptics",
      "expo-keep-awake",
      "expo-linking",
      "expo-modules-core",
      "expo-notifications",
      "expo-permissions",
      "expo-splash-screen",
      "expo-status-bar",
      "expo-structured-headers",
      "expo-updates",
      "expo-updates-interface",
      "react-native",
      "react-native-reanimated",
      "react-native-safe-area-context",
      "react-native-unimodules",
      "sentry-expo",
      "unimodules-app-loader",
      "unimodules-barcode-scanner-interface",
      "unimodules-camera-interface",
      "unimodules-constants-interface",
      "unimodules-face-detector-interface",
      "unimodules-file-system-interface",
      "unimodules-font-interface",
      "unimodules-image-loader-interface",
      "unimodules-permissions-interface",
      "unimodules-sensors-interface",
      "unimodules-task-manager-interface"
    ]
  },
  "installConfig": {
    "hoistingLimits": "none",
    "selfReferences": false
  },
  "jest": {
    "testRegex": "((/(tests|__tests__)((/|/[^_.\n][^/\n]*(/[^_.\n][^/\n]+)*/)[^_.\n][^/\n]*))|((/|/.*/)(([^_.\n][^/\n]*.(spec|test))|(spec|test)))).[jt]sx?$",
    "testEnvironment": "node",
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/es/",
      "<rootDir>/lib/",
      "<rootDir>/out/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/_setup.ts"
    ]
  },
  "jestSonar": {
    "reportPath": ".mkpm/.tmp/reports",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
