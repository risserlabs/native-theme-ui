{
  "name": "@multiverse/webext",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "webext:dev": "cd dist && web-ext run --verbose --browser-console",
    "webext:build": "rm -rf dist && next export && webpack && cd dist/out && rm -rf _next && (grep -rl '/_next' * | xargs sed -i 's|/_next|/next|g')"
  },
  "dependencies": {
    "app": "*",
    "next": "11.1.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.0",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/chrome": "^0.0.163",
    "@types/firefox-webext-browser": "^94.0.0",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "copy-webpack-plugin": "6.2.1",
    "web-ext": "^6.5.0",
    "webpack": "^5.64.0"
  }
}
