{
  "name": "multiverse-example",
  "version": "0.0.1",
  "description": "multiverse example",
  "manifest_version": 2,
  "devtools_page": "devtools.html",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "web_accessible_resources": ["injectable.js"]
}
