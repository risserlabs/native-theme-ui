  "eslintIgnore": [
    "es",
    "lib"
  ],
  "jest": {
    "testRegex": "((/(tests|__tests__)((/|/[^_.\n][^/\n]*(/[^_.\n][^/\n]+)*/)[^_.\n][^/\n]*))|((/|/.*/)(([^_.\n][^/\n]*.(spec|test))|(spec|test)))).[jt]sx?$",
    "testEnvironment": "node",
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/es/",
      "<rootDir>/lib/",
      "<rootDir>/out/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/_setup.ts"
    ]
  },
  "jestSonar": {
    "reportPath": ".mkpm/.tmp/reports",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "loki": {
    "configurations": {
      "chrome.iphone7": {
        "target": "chrome.app",
        "preset": "iPhone 7"
      }
    },
    "diffingEngine": {
      "gm": {},
      "looks-same": {},
      "pixelmatch": {}
    },
    "verboseRenderer": true
  }
