  },
  "devDependencies": {
    "expo-yarn-workspaces": "^1.5.1",
    "patch-package": "^6.4.7"
  },
  "workspaces": [
    "src",
    "platforms/*"
  ],
  "scripts": {
    "postinstall": "patch-package",
    "vscode:compile": "cd platforms/vscode && yarn run vscode:compile",
    "vscode:run-in-browser": "cd platforms/vscode && yarn run vscode:run-in-browser",
    "vscode:watch": "cd platforms/vscode && yarn run vscode:watch"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.1",
    "react-native": "^0.64.2",
    "react-native-web": "^0.17.1"
  }
}
