{
  "name": "multiverse",
  "version": "0.0.1",
  "description": "write once, render anywhere",
  "private": true,
  "homepage": "https://gitlab.com/bitspur/community/multiverse",
  "repository": "git@gitlab.com:bitspur/community/multiverse.git",
  "bugs": {
    "url": "https://gitlab.com/bitspur/community/multiverse",
    "email": "howdy@bitspur.com"
  },
  "license": "Apache-2.0",
  "author": "BitSpur, Inc. <howdy@bitspur.com> (https://bitspur.com)",
  "workspaces": [
    "src",
    "platforms/*"
  ],
  "scripts": {
    "upgrade": "ncu -i && sh -c 'COMMAND=\"ncu -i\" && for i in $(yarn workspaces list | sed \"s|YN0000: \\..*||g\" | sed \"s|YN0000: Done in.*||g\" | cut -d\" \" -f3); do sh -c \"cd $i && $COMMAND\"; done' && yarn",
    "vscode:compile": "cd platforms/vscode && yarn run vscode:compile",
    "vscode:run-in-browser": "cd platforms/vscode && yarn run vscode:run-in-browser",
    "vscode:watch": "cd platforms/vscode && yarn run vscode:watch"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-native": "^0.66.3",
    "react-native-web": "^0.17.5"
  },
  "devDependencies": {
    "@types/node": "^16.11.9",
    "@types/react": "^17.0.35",
    "@types/react-native": "^0.66.5",
    "npm-check-updates": "^12.0.2"
  },
  "packageManager": "yarn@3.1.0",
  "resolutions": {
    "@storybook/addon-react-native-web@0.0.7": "patch:@storybook/addon-react-native-web@npm:0.0.7#.yarn/patches/@storybook-addon-react-native-web-npm-0.0.7-49dddd98a4"
  }
}
